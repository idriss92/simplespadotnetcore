  "scripts": {
    "postinstall": "dotnet restore ./backend  && dotnet restore ./backend.test",
    "test:backend": "cd ./backend.test && dotnet xunit",
    "build": "dotnet build ./backend",
    "migrate": "cd ./backend/ && node ../scripts/create-migration.js && dotnet ef database update",
    "test": "npm run test:backend",
    "start": "cd ./backend && cross-env NODE_PATH=../node_modules/ ASPNETCORE_ENVIRONMENT=Development dotnet watch run",
    "start:release": "npm run prerelease && cd ./backend/bin/Release/netcoreapp2.0/publish/ && dotnet backend.dll"
